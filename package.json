{
  "name": "blanc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/admin"
  ],
  "scripts": {
    "dev": "vite",
    "build": "node scripts/vite-build.mjs",
    "preview": "vite preview",
    "netlify:build": "node scripts/netlify-build.mjs",
    "admin:dev": "npm -w apps/admin run dev",
    "admin:build": "npm -w apps/admin run build",
    "admin:preview": "npm -w apps/admin run preview",
    "server": "node server/index.js",
    "server:dev": "nodemon --watch server --ext js,json --exec \"node server/index.js\"",
    "seed": "node server/scripts/migrate.js",
    "seed:large": "node server/scripts/migrate.js && node server/scripts/seed-large.js",
    "seed:large:reset": "node server/scripts/migrate.js && node server/scripts/seed-large.js --reset",
    "db:init": "node server/scripts/setup-db.js",
    "migrate:mongo-to-pg": "node server/scripts/migrate-mongodb-to-postgres.js",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "bcryptjs": "^2.4.3",
    "busboy": "^1.6.0",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.555.0",
    "nodemailer": "^7.0.11",
    "pg": "^8.13.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.6",
    "recharts": "^3.5.1",
    "serve": "^14.2.0",
    "serverless-http": "^3.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "overrides": {
    "qs": "6.14.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "jsdom": "^26.1.0",
    "mongodb": "^7.0.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.2.4"
  }
}
